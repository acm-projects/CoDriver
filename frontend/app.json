{
  "expo": {
    "name": "codriver",
    "slug": "codriver",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.codriver"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "scheme": "codriver",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "plugins": [
      "./plugins/withAndroidMicrophone",
      "expo-asset"
    ]
  }
}
